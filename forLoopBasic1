#3
for y in range(0, 101, 1):
    if y % 5==0:
        print("Coding")
    else:
        print(y)

#4
odds = 0

for b in range(1, 500001, 2):
    odds += b
    print(odds)

#5
for t in range(2018, 0, -4):
    print(t)

#6
lowNum = 2
highNum = 14893
mult = 69

for h in range(lowNum, highNum, 1):
    if h % mult==0:
        print(h)
